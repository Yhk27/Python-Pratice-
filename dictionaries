


customer = {
    "name": "johnny",
    "age": 50,
    "is_verified": True
}
print(customer["name"])


phone_num = input("Phone: ")
num_conv = {
    "1": "One",
    "2": "Two",
    "3": "Three",
    "4": "Four",
    "5": "Five",
    "6": "Six",
    "7": "Seven",
    "8": "Eight",
    "9": "Nine",
    "0": "Zero",
}
output = ""
for ch in phone_num:
    output +=num_conv.get(ch, "!") + '  '
print(output)
